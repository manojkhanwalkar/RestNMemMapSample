<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	   		   http://www.springframework.org/schema/context
	   		   http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <bean id="ServiceList" class="server.ServiceList">
        <property name="listServices">
            <list>
                <ref bean="RestService" />
            </list>
        </property>

    </bean>




    <bean id="RestService" class="flow.app1.App1Service">
        <property name="name" value="RestService" />
        <property name="restConfigName" value="/Users/mkhanwalkar/RestNMemMapSample/src/main/java/config/app1.yml" />

    </bean>


</beans>

